{
  "cli": {
    "version": ">= 5.4.0"
  },
  "build": {
    "development": {
      "android": {
        "buildType": "apk"
      },
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "env": {
        "EXPO_PUBLIC_API_URL": "https://roomait-backend-production.up.railway.app"
      }
    },
    "preview": {
      "android": {
        "buildType": "apk"
      },
      "distribution": "internal",
      "channel": "preview",
      "env": {
        "EXPO_PUBLIC_API_URL": "https://roomait-backend-production.up.railway.app"
      }
    },
    "production": {
      "android": {
        "buildType": "apk"
      },
      "channel": "production",
      "env": {
        "EXPO_PUBLIC_API_URL": "https://roomait-backend-production.up.railway.app"
      }
    }
  },
  "submit": {
    "production": {}
  }
}