{
  "name": "roomait-mobile",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "build:all": "eas build --platform all",
    "submit:android": "eas submit --platform android",
    "submit:ios": "eas submit --platform ios",
    "deploy": "node ../../scripts/deploy-mobile.js",
    "deploy:dev": "eas build --platform all --profile development",
    "deploy:preview": "eas build --platform all --profile preview",
    "deploy:prod": "eas build --platform all --profile production",
    "update": "eas update --auto",
    "update:dev": "eas update --auto --branch development",
    "update:preview": "eas update --auto --branch preview",
    "update:prod": "eas update --auto --branch production"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "expo": "~54.0.7",
    "expo-auth-session": "~7.0.8",
    "expo-blur": "^15.0.7",
    "expo-camera": "~17.0.7",
    "expo-crypto": "~15.0.7",
    "expo-font": "^14.0.8",
    "expo-linear-gradient": "^15.0.7",
    "expo-router": "~6.0.4",
    "expo-secure-store": "^15.0.7",
    "expo-splash-screen": "^31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-web-browser": "^15.0.7",
    "nativewind": "^4.2.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-reanimated": "~4.1.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "^0.21.0",
    "react-native-worklets": "^0.5.1",
    "tailwindcss": "^3.4.0",
    "expo-updates": "~29.0.10"
  },
  "devDependencies": {
    "@types/react": "~19.1.10",
    "typescript": "~5.9.2"
  },
  "private": true
}
